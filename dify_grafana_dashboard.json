            "type": "grafana-postgresql-datasource",
            "uid": "${DS_DIFY-POSTGRESQL-DATASOURCE}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "select apps.name            as app_name,\n       apps.id              as app_id,\n       message_counts.count as message_count\nfrom (SELECT COUNT(1) AS count,\n             app_id\n      FROM messages\n      GROUP BY app_id\n      order by count desc\n      limit 10) as message_counts\n         JOIN apps on message_counts.app_id = apps.id\norder by message_counts.count desc;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "name": "COUNT",
                "parameters": [
                  {
                    "name": "id",
                    "type": "functionParameter"
